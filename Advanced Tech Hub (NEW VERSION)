local Tab = Window:AddTab({
	Title = "Player",
	Icon = "target"
})

Tab:AddButton({
	Title = "Normal Run",
	Tip = "Makes You Run Normally.",
	Callback = function()
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
	end,
})

Tab:AddButton({
	Title = "Fast Run",
	Tip = "Makes You Run Pretty Fast!",
	Callback = function()
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 50
	end,
})

Tab:AddButton({
	Title = "SUPERHUMAN RUN",
	Tip = "Makes You Run LIKE CAPTAIN AMERICA.",
	Callback = function()
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 120
	end,
})

Tab:AddButton({
	Title = "INF Speed",
	Tip = "Makes You Run Infenentilly fast!",
	Callback = function()
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 90000
	end,
})

Tab:AddButton({
	Title = "Normal Jump",
	Tip = "Makes You Jump Like A Normal Person!",
	Callback = function()
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
	end,
})

Tab:AddButton({
	Title = "Powerful Jump",
	Tip = "Makes You Jump HIGH!",
	Callback = function()
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = 100
	end,
})

Tab:AddButton({
	Title = "Powerful Jump",
	Tip = "Makes You Jump HIGH!",
	Callback = function()
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = 100
	end,
})

Tab:AddButton({
	Title = "EXTREME JUMP",
	Tip = "JUMP TO SPACE!",
	Callback = function()
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = 1009
	end,
})

Tab:AddButton({
	Title = "INF JUMP",
	Tip = "JUMP OUT OF THE MULTIVERSE!",
	Callback = function()
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = 900000000
	end,
})


Tab:AddButton({
	Title = "Suicide",
	Tip = "Kill Yourself!",
	Callback = function()
    game.Players.LocalPlayer.Character.Head:Destroy()
if game.Players.LocalPlayer.Character.Humanoid.Health < 5 then 
    local deathmanok = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").position
    wait(0.5)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(deathmanok)
	end
	end,
})


